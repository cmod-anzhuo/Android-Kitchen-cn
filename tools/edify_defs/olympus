
# Motorola Atrix

change_mnt=yes

param1=ext3
param2=EMMC

param1_sdcard=vfat
param2_sdcard=MTD

use_cache_not_tmp=yes

sys_mnt=\/dev\/block\/mmcblk0p12
cache_mnt=\/dev\/block\/mmcblk0p15
boot_mnt=\/dev\/block\/mmcblk0p11
data_mnt=\/dev\/block\/mmcblk0p16
sdcard_mnt=\/dev\/block\/mmcblk0p18
webtop_mnt=\/dev\/block\/mmcblk0p13
