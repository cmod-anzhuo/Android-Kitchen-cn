if [ ! -d WORKING_* ]
then
  echo "未找到工作目录！"
  exit 0
fi

cd WORKING_*

if [ ! -e boot.img ]
then
  echo "未找到 boot.img！"
  exit 0
fi

cmd_line=`od -A n --strings -j 64 -N 512 boot.img`

echo $cmd_line
